{
"name":"Inject DOM",
"description":"http://stackoverflow.com/questions/14068879",
"version":"1",
"manifest_version":2,

"browser_action": {
		"default_icon": "icon.png"
},
"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"css": ["zelda.css"],
		"js": ["jquery-1.11.3.js", "myscript.js"]
	}
],

"background": {
      "scripts": ["myscript.js"]
  },

"permissions": ["<all_urls>"]
}
 

